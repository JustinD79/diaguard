# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Stripe Configuration
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Stripe Price IDs (create these in Stripe Dashboard)
EXPO_PUBLIC_STRIPE_GOLD_MONTHLY_PRICE_ID=price_xxx
EXPO_PUBLIC_STRIPE_DIAMOND_MONTHLY_PRICE_ID=price_xxx
EXPO_PUBLIC_STRIPE_DIAMOND_YEARLY_FIRST_PRICE_ID=price_xxx
EXPO_PUBLIC_STRIPE_DIAMOND_YEARLY_RENEWAL_PRICE_ID=price_xxx
EXPO_PUBLIC_STRIPE_FAMILY_MONTHLY_PRICE_ID=price_xxx

# AI API Keys (use one or both)
EXPO_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key_here
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Food Database APIs (optional but recommended)
EXPO_PUBLIC_USDA_API_KEY=your_usda_api_key_here
EXPO_PUBLIC_NUTRITIONIX_APP_ID=your_nutritionix_app_id_here
EXPO_PUBLIC_NUTRITIONIX_APP_KEY=your_nutritionix_app_key_here

# Dexcom CGM Integration
EXPO_PUBLIC_DEXCOM_CLIENT_ID=your_dexcom_client_id_here
EXPO_PUBLIC_DEXCOM_CLIENT_SECRET=your_dexcom_client_secret_here
EXPO_PUBLIC_DEXCOM_REDIRECT_URI=your_app_url/dexcom/callback
EXPO_PUBLIC_DEXCOM_USE_SANDBOX=true

# App Configuration
EXPO_PUBLIC_APP_URL=http://localhost:8081
EXPO_PUBLIC_API_URL=http://localhost:8081

# Feature Flags
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false
